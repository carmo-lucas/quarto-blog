@article{chun2010,
  ids = {chun2010a},
  title = {Stabilization and Control of Rheological Properties of {{Fe2O3}}/{{Al}}({{OH}})3-Rich Colloidal Slurries under High Ionic Strength and {{pH}}},
  author = {Chun, Jaehun and Poloski, Adam P. and Hansen, Erich K.},
  date = {2010},
  journaltitle = {Journal of Colloid and Interface Science},
  volume = {348},
  number = {1},
