@article{gilbert2013,
  ids = {gilbert2013c},
  title = {Rheological and Textural Characterization of Cosmetic Emulsions Containing Natural and Synthetic Polymers: Relationships between Both Data},
  author = {Gilbert, Laura and Picard, Céline and Savary, Géraldine and Grisel, Michel},
  date = {2013},
  journaltitle = {Colloids and Surfaces A: Physicochemical and Engineering Aspects},
